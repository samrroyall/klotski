{
  "name": "klotski",
  "version": "0.3.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.6.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@sentry/react": "^7.106.0",
    "axios": "^1.6.7",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^2.6.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./ --fix --max-warnings=0",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json)\""
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "bracketSpacing": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "plugins": [
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "prettier",
      "react-app"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error",
      "import/first": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
